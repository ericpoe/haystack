# See http://EditorConfig.org for more information

# top-most EditorConfig file
root = true

# Every File
[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
indent_style = space
indent_size = 4

[*.{yml,json,md}]
indent_size = 2
insert_final_newline = false
